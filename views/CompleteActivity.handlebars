<body>

    <div class="background">
<i class="material-icons" style="float:left;margin-left: 28px;cursor: pointer;color: rgba(133,133,133,1);padding-top: 10px; text-align: center;"><a href="activities" style="text-decoration: none">arrow_back</i></a>
    <div class="title">

  <h3 style="font-weight: 400">Math</h3>
</div>
<div class="timeelapsed">
  <p>Time Elapsed: 0 min</p>
</div>
<script type="text/javascript">
  function startTimer(duration, display) {
    var timer = duration, minutes, seconds;
    setInterval(function () {
        minutes = parseInt(timer / 60, 10)
        seconds = parseInt(timer % 60, 10);

        minutes = minutes < 10 ? "0" + minutes : minutes;
        seconds = seconds < 10 ? "0" + seconds : seconds;

        display.textContent = minutes + ":" + seconds;

        if (--timer < 0) {
            timer = duration;
        }
    }, 1000);
}

function begin() {
    var fiveMinutes = 60 * 25,
        display = document.querySelector('#time');
    startTimer(fiveMinutes, display);
};
</script>
  
  <div class="card">
      <div class="container">
        <div><span class="timer" id="time">25:00</span></div>
        <button type="button" onclick="begin()">Start Work</button>
      </div>
    </div>


        <div class="button_finish">
    <button onclick="javascrtpt:window.location.href='activities'" class="pure-material-button-contained">Finish</button>
</div>



    <div class="fab" id="fabAExercise"> <i class="material-icons"><a href="ExercisesButton">spa</i></a>
    </div>


</div>
